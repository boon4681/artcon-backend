{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "$model": [
        "./src/database/index.ts"
      ],
      "$argv": [
        "./src/args.ts"
      ],
      "$constants": [
        "./src/constants.ts"
      ],
      "$helper": [
        "./src/helper.ts"
      ],
      "$types":[
        "./src/types.ts"
      ],
      "$middlewares/*":[
        "./src/middleware/*"
      ],
      "$bucket":[
        "./src/bucket/bucket.ts"
      ]
    }
  }
}